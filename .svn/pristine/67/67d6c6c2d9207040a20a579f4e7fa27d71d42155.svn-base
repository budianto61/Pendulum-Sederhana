							<script>
						function budianto(){
							atur_window("animasi/projects/animasi.html", "Animation", 1600, 1200);
						}
						function atur_window(url, title, w, h){
								var kiri = (screen.width/2)-(w/2);
								var atas = (screen.height/2)-(h/2);
							return window.open(url, title, 'toolbar=no, directories=no, status=no, menubar=no, width=1600, height=1200, top='+atas+', left='+kiri);
